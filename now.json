{
  "version": 2,
  "alias": "gql-all-things",
  "name": "gql-all-things",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build"
    }
  ],
  "routes": [
    {
      "src": "^/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "^/snippets/(.*)",
      "dest": "/snippets/$1"
    },
    {
      "src": "index.html",
      "dest": "/index.html"
    }
  ]
}
